<div id="tag_list">
  <h5>Tags</h5>
  <ul>
    <!-- google_ad_section_start -->
    <%= tag_links(@tags[:exclude], :prefix => "-", :hide_unsafe_posts => hide_unsafe_posts?) %>
    <%= tag_links(@tags[:include], :hide_unsafe_posts => hide_unsafe_posts?) %>
    <%= tag_links(Tag.find_related(@tags[:related].to_a), :hide_unsafe_posts => hide_unsafe_posts?) %>
    <!-- google_ad_section_end -->
  </ul>
</div>
