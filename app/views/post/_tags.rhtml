<div>
  <h5>Tags</h5>
  <ul id="tag-sidebar">
    <%= tag_links(@tags[:exclude], :prefix => "-", :hide_explicit => hide_explicit?) %>
    <%= tag_links(@tags[:include], :hide_explicit => hide_explicit?) %>
    <%= tag_links(Tag.find_related(@tags[:related].to_a), :hide_explicit => hide_explicit?) %>
  </ul>
</div>
